<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>layout 管理界面大布局示例 - Layui</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/lib/node_modules/layui/dist/css/layui.css" rel="stylesheet">
</head>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/node_modules/layui/dist/layui.js"></script>
<script src="~/lib/node_modules/axios/dist/axios.min.js"></script>
<body>
    <div class="layui-layout layui-layout-admin">
        <div class="layui-header">
            <div class="layui-logo layui-hide-xs layui-bg-black">layout demo</div>
            <!-- 头部区域（可配合layui 已有的水平导航） -->
            @await Html.PartialAsync("_HeaderPartial")
        </div>
        <div class="layui-side layui-bg-black">
            <div class="layui-side-scroll">
                <!-- 左侧导航区域（可配合layui已有的垂直导航） -->
                @await Html.PartialAsync("_MenuPartial")
            </div>
        </div>
        <div class="layui-body">
            <!-- 内容主体区域 -->
            @RenderBody()
            <br><br>
        </div>
        <div class="layui-footer">
            <!-- 底部固定区域 -->
            @await Html.PartialAsync("_FooterPartial")

        </div>
    </div>

    @*<script src="//cdn.staticfile.org/layui/2.8.18/layui.js"></script>*@
</body>
</html>
